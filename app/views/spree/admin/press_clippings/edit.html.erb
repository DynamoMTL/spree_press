<h1>Editing Press Clippings</h1>

<%= form_for(:press_clipping, :url => object_url, :html => { :method => :put, :multipart => true }) do |f| -%>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%= submit_tag t("update") %> or <%= link_to t("cancel"), collection_url %>
  </p>
<% end %>