<h1>New Press Clipping</h1>

<%= error_messages_for :press_clipping %>

<% form_for(:press_clipping, :url => collection_url, :html => { :multipart => true }) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%= submit_tag t("create") %> or <%= link_to t("cancel"), collection_url %>
  </p>
<% end %>